{
  "name": "next-exam-teacher",
  "version": "1.1.0",
  "private": true,
  "main": "dist/main/main.mjs",
  "author": {
    "name": "Thomas Michael Weissel",
    "email": "valueerror@gmail.com",
    "url": "https://next-exam.at"
  },
  "homepage": "https://next-exam.at",
  "license": "GPL-3.0",
  "description": "An exam managment tool for digital exams",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others --prefix-colors \"green,blue\" \"node bip-demo-api/api.js\" \"npm run prebuild && node scripts/watch.mjs\"",
    "clean": "rm -rf ./dist",
    "cleanwin": "if exist dist rmdir /s /q dist",
    "prebuild": "node scripts/prebuild.js",
    "protect:main": "node scripts/protect-main.mjs",
    "build:linux": "npm run prebuild && npm run clean && node scripts/build.mjs && npm run protect:main && electron-builder -l --publish never",
    "build:mac:x64": "npm run prebuild && npm run clean && node scripts/build.mjs && npm run protect:main && electron-builder -m dmg:x64 --publish never",
    "build:mac:arm64": "npm run prebuild && npm run clean && node scripts/build.mjs && npm run protect:main && electron-builder -m dmg:arm64 --publish never",
    "build:win:portable": "npm run prebuild && npm run cleanwin && node scripts/build.mjs && npm run protect:main && electron-builder -w portable:x64",
    "build:win:msi": "npm run prebuild && npm run cleanwin && node scripts/build.mjs && npm run protect:main && electron-builder -w msi:x64"
  },
  "engines": {
    "node": "22.19.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@types/jquery": "^3.5.30",
    "@types/screenshot-desktop": "^1.12.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "concurrently": "^9.1.2",
    "electron": "38.1.2",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.23.1",
    "javascript-obfuscator": "^4.1.1",
    "nodemon": "^3.1.5",
    "sass": "^1.93.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-resolve": "^2.5.2"
  },
  "env": {
    "//": "Used in build scripts",
    "PORT": 3002
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ],
